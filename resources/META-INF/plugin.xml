<idea-plugin>
  <id>marseille-progressbar</id>
  <name>Marseille Progress Bar</name>
  <version>1.15</version>
  <idea-version since-build="233" until-build="243.*"/>
  <vendor email="tudorsarghiuta@gmail.com">Tudor Sarghiuta</vendor>

  <description><![CDATA[
      Olympique de Marseille themed progress bar for IntelliJ IDEA based on Nyan Progress Bar plugin from Dmitry Batkovich
    ]]></description>

  <change-notes><![CDATA[
        <ul>
          <dt><b>1.15</b></dt>
          <li>First release</li>
        </ul>
    ]]></change-notes>
  <depends>com.intellij.modules.platform</depends>

  <extensions defaultExtensionNs="com.intellij">
  </extensions>

  <actions>
  </actions>

  <applicationListeners>
    <listener class="MarseilleApplicationComponent" topic="com.intellij.ide.ui.LafManagerListener"/>
    <listener class="MarseilleApplicationComponent" topic="com.intellij.openapi.application.ApplicationActivationListener"/>
  </applicationListeners>
</idea-plugin>